
<%- include('layout/base') -%>
<%- include('layout/nav') -%>

  <div class="container">
    <h4 class="h4-user-name-rep"><%= user %>'s Github Tree</h4>
    <h6 class="h6-total-repo">Total Repositories : <%=length%></h6>
    <div class="row">
      <div class="col-md-12">
        <div class="main-timeline2">
        <% for(var i = 0;i<repos.length;i++) {%>
          <div class="timeline">
            <span class="icon fa fa-github"></span>
            <a href="<%=repos[i]['url']%>" target="_blank" class="timeline-content">
              <h3 style="display:inline-block;text-decoration:underline;" class="title"><%=repos[i]['fullname']%></h3>
              <i style="display:inline-block; margin:0 10px;" class="fa fa-external-link fa-lg" aria-hidden="true"></i>
              <p class="last-updated-paragraph" >Last updated: <%=repos[i]['update']%></p>
              <p class="description">
                <%=repos[i]['des']%>
              </p>
            </a>
          </div>
        <%}%>
        </div>
      </div>
    </div>
  </div>
<%- include('layout/footer') -%>
